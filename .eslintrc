{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "plugins": [ "babel" ],
  "rules": {
    "max-len": [2, { "code": 120, "ignoreUrls": true }],
    "import/prefer-default-export": 0,
    "react/destructuring-assignment": 0
  },
  "settings": {
    "react": {
      "pragma": "React"
    },
    "import/resolver": {
      "webpack": {
        "config": "./configs/webpack-dev.js"
      }
    }
  }
}
